modules = ["nodejs-20", "web", "postgresql-16"]
run = "node server/server.js"

[nix]
channel = "stable-24_05"

[deployment]
run = "node server/server.js"
deployTrigger = ["main"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 3000
externalPort = 80

[[ports]]
localPort = 5000
externalPort = 5000

[workflows]
runButton = "Start Server"

[[workflows.workflow]]
name = "Start Server"
author = 10236786
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node server/server.js"

